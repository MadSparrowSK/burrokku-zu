                                </Button>
                                <Button  Style="{StaticResource Button_task}" Height="16" Name="load_3" Margin="0"  Click="load_Click" >
                                    <Image Source="Resources/GraySparowDown.png" Width="16" Height="15" Margin="0,1,0,0" />
                                </Button>
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <Button  Style="{StaticResource Button_task}"  Margin="0,5,2,5" Width="175" Click="Open_task" Name="task_4">
                                <TextBlock Text="Задание №4" Margin="42,0,0,0"/>
                            </Button>
                            <StackPanel Margin="0,5,0,0">
                                <Button  Style="{StaticResource Button_task}" Height="16" Name="save_4" Margin="0,0,0,1"  Click="save_Click" >
                                    <Image Source="Resources/GraySparowUp.png" Width="16" Height="15" Margin="0,0,0,1" />
                                </Button>
                                <Button  Style="{StaticResource Button_task}" Height="16" Name="load_4" Margin="0"  Click="load_Click" >
                                    <Image Source="Resources/GraySparowDown.png" Width="16" Height="15" Margin="0,1,0,0" />
                                </Button>
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <Button  Style="{StaticResource Button_task}"  Margin="0,5,2,5" Width="175" Click="Open_task" Name="task_5">
                                <TextBlock Text="Задание №5" Margin="42,0,0,0"/>
                            </Button>
                            <StackPanel Margin="0,5,0,0">
                                <Button  Style="{StaticResource Button_task}" Height="16" Name="save_5" Margin="0,0,0,1"  Click="save_Click" >
                                    <Image Source="Resources/GraySparowUp.png" Width="16" Height="15" Margin="0,0,0,1" />
                                </Button>
                                <Button  Style="{StaticResource Button_task}" Height="16" Name="load_5" Margin="0"  Click="load_Click" >
                                    <Image Source="Resources/GraySparowDown.png" Width="16" Height="15" Margin="0,1,0,0" />
                                </Button>
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <Button  Style="{StaticResource Button_task}"  Margin="0,5,2,5" Width="175" Click="Open_task" Name="task_6">
                                <TextBlock Text="Задание №6" Margin="42,0,0,0"/>
                            </Button>
                            <StackPanel Margin="0,5,0,0">
                                <Button  Style="{StaticResource Button_task}" Height="16" Name="save_6" Margin="0,0,0,1"  Click="save_Click" >
                                    <Image Source="Resources/GraySparowUp.png" Width="16" Height="15" Margin="0,0,0,1" />
                                </Button>
                                <Button  Style="{StaticResource Button_task}" Height="16" Name="load_6" Margin="0"  Click="load_Click" >
                                    <Image Source="Resources/GraySparowDown.png" Width="16" Height="15" Margin="0,1,0,0" />
                                </Button>
                            </StackPanel>
                        </StackPanel>
                        
                    </StackPanel>
                    <StackPanel x:Name="SP3" Orientation="Horizontal" Margin="0,0,0,0" >
                        <Label Style="{StaticResource Button_right_sidebar}"
                               MouseDown="Button_right_sidebar_MouseDown"
                               MouseUp="Button_right_sidebar_MouseUp"
                               MouseEnter="Button_right_sidebar_Enter"
                               MouseLeave="Button_right_sidebar_Leave"
                               Name="LabsSwitch"
                               Width= "90"
                               Padding="1,0">
                            <Label.Content>
                                <TextBlock Text="Лабораторная" Margin="0,0,0,0"
                           FontSize="13"
                       />
                            </Label.Content>
                        </Label>
                        <Label Style="{StaticResource Button_right_sidebar}"
                               MouseDown="Button_right_sidebar_MouseDown"
                               MouseUp="Button_right_sidebar_MouseUp"
                               MouseEnter="Button_right_sidebar_Enter"
                               MouseLeave="Button_right_sidebar_Leave" 
                               Width="53"
                               Padding="1,0,0,0"
                               Name="TermSwitch"
                               >

                            <Label.Content>
                                <TextBlock Text="Семестр" Margin="0,0,0,0"
                           FontSize="13"
                       />
                            </Label.Content>


                        </Label>
                        
                    </StackPanel>
                    
                </StackPanel>

            </Grid>
            <Grid Name="FileZone" Background="#FF41403E" Height="50" Width="130" Margin="9,24,0,0"
                  Grid.Row="1" Grid.Column="1" 
                  VerticalAlignment="Top"
                  HorizontalAlignment="Left"
                  Panel.ZIndex="900"
                  MouseLeave="FileZone_MouseLeave"
                  Visibility="Hidden">
                <StackPanel Orientation="Vertical" HorizontalAlignment="Left" Width="130" Background="#FF2E2E30">
                    <Label   Style="{StaticResource Button}" MouseDown="DownLoad" Name="btnLoad">
                        <Label.Content>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Загрузить"/>
                                <TextBlock Text="Ctrl+L" Margin="52,0,0,0" Foreground="Gray"/>
                            </StackPanel>
                        </Label.Content>
                    </Label>
                    <Label  Style="{StaticResource Button}"  MouseDown="CutDownSave" Name="btnShortSave">
                        <Label.Content>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Сохранить"/>
                                <TextBlock Text="Alt+S" Margin="48,0,0,0" Foreground="Gray"/>
                            </StackPanel>
                        </Label.Content>
                    </Label>
                    <Label  Style="{StaticResource Button}" MouseDown="DownSave" Name="btnSave">
                        <Label.Content>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Сохранить как"/>
                                <TextBlock Text="Ctrl+S" Margin="28,0,0,0" Foreground="Gray"/>
                            </StackPanel>
                        </Label.Content>
                    </Label>
                </StackPanel>
            </Grid>
            <Grid Name="EditZone" Background="#FF2E2E30"  Height="50" Width="130" Margin="59,24,0,0"
                  Grid.Row="1" Grid.Column="1" 
                  VerticalAlignment="Top"
                  HorizontalAlignment="Left"
                  Panel.ZIndex="900"
                  MouseLeave="EditZone_MouseLeave"
                  Visibility="Hidden">
                <StackPanel Orientation="Vertical" HorizontalAlignment="Left" Width="130">
                    <Label  Style="{StaticResource Button}"  Name="btnPrev" MouseDown="prev">
                        <Label.Content>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Отменить"/>
                                <TextBlock Text="Ctrl+Z" Margin="50,0,0,0" Foreground="Gray"/>
                            </StackPanel>
                        </Label.Content>
                    </Label>
                    <Label  Style="{StaticResource Button}" Name="btnNext" MouseDown="next">
                        <Label.Content>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Восстановить"/>
                                <TextBlock Text="Ctrl+Y" Margin="33,0,0,0" Foreground="Gray"/>
                            </StackPanel>
                        </Label.Content>
                    </Label>
                    <Label  Style="{StaticResource Button}"  Name="btnDelete" MouseDown="inTrash">
                        <Label.Content>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Удалить все"/>
                                <TextBlock Text="Alt+C" Margin="43,0,0,0" Foreground="Gray"/>
                            </StackPanel>
                        </Label.Content>
                    </Label>

                </StackPanel>

            </Grid>
