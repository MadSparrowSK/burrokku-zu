                  Grid.Row="1" Grid.Column="2" 
                  VerticalAlignment="Top"
                  HorizontalAlignment="Right"
                  Panel.ZIndex="900"
                  MouseLeave="TermZone_MouseLeave"
                  Visibility="Hidden"
                  >
                <Polyline Points="52,0 100,0 100,35 0,35 0,0" StrokeThickness="2">
                    <Polyline.Stroke>
                        <LinearGradientBrush EndPoint="0.3,1.4"   StartPoint="0.2,0" >
                            <GradientStop Color="#00069B" Offset="0"/>
                            <GradientStop Color="#BA48B6" Offset="1"/>
                        </LinearGradientBrush>
                    </Polyline.Stroke>
                </Polyline>
                <StackPanel Orientation="Vertical" HorizontalAlignment="Left" Width="130">
                    <StackPanel Orientation="Horizontal" >
                        <CheckBox Style="{StaticResource CheckBox}"  Name="Term_1" Checked="Checked" Unchecked="Checked" IsChecked="True" />
                        <TextBlock Text="Семестр №1" Margin="5,0,0,0"  Foreground="#FF303030" FontSize="13"/>
                    </StackPanel>
                    <Polyline Points="0,0 130,0" StrokeThickness="1" >
