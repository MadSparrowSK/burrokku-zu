                                <Image Source="Resources/GraySparow.png" Width="16" Height="15" Margin="0,0,0,0" x:Name="strelka"/>
                            </Label.Content>
                        </Label>
                        

                    </StackPanel>
                    <StackPanel  Name="SP2">
                        <StackPanel Orientation="Horizontal">
                            <Button  Style="{StaticResource Button_task}"  Margin="0,5,2,5" Width="175" Click="Open_task" Name="task_1">
                                <TextBlock Text="Задание №1" Margin="42,0,0,0"/>
                            </Button>
                            <StackPanel Margin="0,5,0,0">
                                <Button  Style="{StaticResource Button_task}" Height="16" Name="save_1" Margin="0,0,0,1" Click="save_Click" >
                                    <Image Source="Resources/GraySparowUp.png" Width="16" Height="15" Margin="0,0,0,1" />
                                </Button>
                                <Button  Style="{StaticResource Button_task}" Height="16" Name="load_1" Click="load_Click" Margin="0"  >
                                    <Image Source="Resources/GraySparowDown.png" Width="16" Height="15" Margin="0,1,0,0" />
                                </Button>
                            </StackPanel>
                        </StackPanel>

                        <StackPanel Orientation="Horizontal">
                            <Button  Style="{StaticResource Button_task}"  Margin="0,5,2,5" Width="175" Click="Open_task" Name="task_2">
                                <TextBlock Text="Задание №2" Margin="42,0,0,0"/>
                            </Button>
